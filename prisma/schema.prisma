generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Card {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  fillColor   String
  createdAt   DateTime @default(now())
}
